                                 <!--<t t-esc="product.categ_id"/>-->
                                 <t t-esc="product.list_price"/>
                                 <!--<t t-esc="product.amount"/>-->
                                 <!--<t t-esc="product.seller"/>-->
                             </p>
                          </t>
                        </div>
                    </div>
                 </t>
        </template>
    </data>
</openerp>